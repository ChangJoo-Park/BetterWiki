{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "max-line-length": [
            true,
            {
                "limit": 120,
                "ignore-pattern": "^import |^export {(.*?)}"
            }
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "src/migration"
        ]
    }
}
